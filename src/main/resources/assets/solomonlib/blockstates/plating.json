{
	"multipart": [
		{
			"when": {
				"down": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"uvlock": true
			}
		},
		{
			"when": {
				"up": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"x": 180,
				"uvlock": true
			}
		},
		{
			"when": {
				"south": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"x": 90,
				"uvlock": true
			}
		},
		{
			"when": {
				"west": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"x": 90,
				"y": 90,
				"uvlock": true
			}
		},
		{
			"when": {
				"north": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"x": 90,
				"y": 180,
				"uvlock": true
			}
		},
		{
			"when": {
				"east": "true"
			},
			"apply": {
				"model": "solomonlib:block/plating",
				"x": 90,
				"y": 270,
				"uvlock": true
			}
		}
	]
}
